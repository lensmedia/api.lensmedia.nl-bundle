{
    "name": "lensmedia/api.lensmedia.nl-bundle",
    "description": "Symfony bundle for to work with our main LENS API",
    "license": "proprietary",
    "type": "symfony-bundle",
    "version": "2.0.0",
    "require": {
        "php": "^8.1",
        "beberlei/doctrineextensions": "dev-master",
        "brick/math": "^0.11.0",
        "brick/money": "^0.8.0",
        "creof/doctrine2-spatial": "^1.2",
        "egulias/email-validator": "^4.0",
        "giggsey/libphonenumber-for-php": "^8.0",
        "sendinblue/api-v3-sdk": "^8.4",
        "symfony/cache": "^6.0",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/orm-pack": "^2.3",
        "symfony/security-core": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/validator": "^6.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.30",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Lens\\Bundle\\LensApiBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "scripts": {
        "fix": "php vendor/bin/php-cs-fixer fix src"
    }
}
