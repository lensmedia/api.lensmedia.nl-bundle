{
    "name": "lensmedia/api.lensmedia.nl-bundle",
    "description": "Symfony bundle for to work with our main LENS API",
    "license": "proprietary",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.4",
        "ext-openssl": "*",
        "beberlei/doctrineextensions": "*",
        "brick/math": "*",
        "brick/money": "*",
        "egulias/email-validator": "^4.0",
        "getbrevo/brevo-php": "^2.0",
        "giggsey/libphonenumber-for-php": "^9.0.20",
        "symfony/cache": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/orm-pack": "*",
        "symfony/security-core": "*",
        "symfony/serializer": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "lensmedia/symfony-meili-search": "dev-main"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.30",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Lens\\Bundle\\LensApiBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": false
        }
    },
    "scripts": {
        "fix": "php vendor/bin/php-cs-fixer fix src"
    },
    "recommend": {
        "lensmedia/symfony-meili-search": "If you are editing lens-api data, you should install this package to automatically update the search indexes on entity changes"
    }
}
